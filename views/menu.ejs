<script type="text/javascript">
function cerrar(){
	alertify.confirm("<p>Desea Cerrar Sesion? </p>",function (e){
	if (e) {
		location.href="/cerrarsesion";
	} 
	else{
		return false;
	};		
	});
}


	var themes = {
    "celerun" : "/../css/temas/celerun.css",
    "flaty" : "/../css/temas/flaty.css",
    "paper" : "/../css/temas/paper.css",
    "yeti" : "/../css/temas/yeti.css",
    "united" : "/../css/temas/united.css",
	}
	$(function(){
   	var themesheet = $('<link href="'+themes['paper']+'" rel="stylesheet" />');
    themesheet.appendTo('head');

    	$('.theme-link').click(function(){
       	var themeurl = themes[$(this).attr('data-theme')]; 
        themesheet.attr('href',themeurl);
    });
});
</script>	

